[color]
	branch = auto
	diff = auto
	status = auto
	interactive = auto

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green

[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold

[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
